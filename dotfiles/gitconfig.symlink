[user]
	name = Paul Woolcock
	email = paul@woolcock.us
[push]
    default = upstream

[core]
	editor = /usr/bin/vim

[alias]
    co = checkout
    cp = cherry-pick
    discard = checkout --
    cleanall = clean -xfd
    dif = diff --patience
    st = status -uno

[filter.ignoreline]
    clean = "sed '/#ignoreline$/'d"
    smudge = "cat"
